<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seniorproject.sauclubmanager" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/s_a_icon"
        android:label="@string/app_name"
        android:theme="@style/Theme.Seniorproj"
        android:name=".myApplication">

        <activity
            android:name=".com.seniorproject.utilities.DispatchActivity"
            android:label="SAU Clubs"
            android:noHistory="true">
            <!--android:windowSoftInputMode="adjustResize|stateVisible"-->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.parse.ui.ParseLoginActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop">
            <meta-data
                android:name="com.parse.ui.ParseLoginActivity.PARSE_LOGIN_ENABLED"
                android:value="true"/>
            <!-- Added these options below to customize the login flow -->
            <meta-data
                android:name="com.parse.ui.ParseLoginActivity.APP_LOGO"
                android:resource="@drawable/s_a_icon"/>
            <meta-data
                android:name="com.parse.ui.ParseLoginActivity.FACEBOOK_LOGIN_ENABLED"
                android:value="false"/>
            <meta-data
                android:name="com.parse.ui.ParseLoginActivity.TWITTER_LOGIN_ENABLED"
                android:value="false"/>
            <meta-data
                android:name="com.parse.ui.ParseLoginActivity.PARSE_LOGIN_EMAIL_AS_USERNAME"
                android:value="true"/>
        </activity>

        <activity android:name = ".HomeActivity"
                  android:label="My Dashboard"/>
        <activity android:name = ".unAuthHomeActivity"
            android:label="My Dashboard"/>
        <activity android:name=".DashboardActivity"/>
        <activity android:name = ".clubs"
                  android:label="Clubs"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".user_profile"
                  android:label="My Profile"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".myfeed"
                  android:label="My Feed"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".myevents"
                  android:label="My Events"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".sa"
                  android:label="Student Association"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".sa_senate_"
                  android:label="S. A. Senate"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".ClubActivity"
                  android:label="S. A. Clubs"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".settings"
                  android:label="Settings"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".report"
                  android:label="Send Report"
                  android:icon="@drawable/title_home_default"/>
        <activity android:name=".reg_login"
                  android:label="@string/reg_text" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />\
</manifest>
